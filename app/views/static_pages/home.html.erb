<% if logged_in? %>
  <div class="row">
    <div class="col-md-12">
      <h1>Newsletter</h1> 
      <%= render 'shared/feed' %>
    </div>
  </div>
<% else %>
  <div class="center jumbotron">
    <h1>Welcome to the Newsletter</h1>
    
    
    <%= link_to "Sign up now", signup_path, class: "btn btn-lg btn-primary" %>  
  </div>

<% end %>
